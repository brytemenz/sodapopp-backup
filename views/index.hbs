<section>
    <div id="carouselExampleCaptions" class="carousel slide">
  <div class="carousel-indicators">
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
  </div>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="/images/collaboration sunset.webp" class="d-block w-100" alt="...">
      <div class="carousel-caption d-none d-md-block">
        <h1>Expand your mind. Develop projects. With Sodapopp!
        </h1>
      </div>
    </div>
    <div class = "carousel-item">
    
      <img src="/images/pic2.jpeg" class="d-block w-100" alt="...">
      
      <div class="carousel-caption d-none d-md-block">
        <h1>Sodapopp is the easy way to share and collaborate on projects. Join us now!
        </h1>
        
      </div>
    </div>
    <div class="carousel-item">
      <img src="/images/pic1.jpeg" class="d-block w-100" alt="...">
      <div class="carousel-caption d-none d-md-block">
        <h1>Upload your projects. Find collaborators. Next level teamwork!
        </h1>
      </div>
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>
</section>
<br>
<br>
<section>
  <h3 class="row justify-content-center">Projects made by awesome students for you to collaborate on!</h3>
  <br>
  <hr>

<div class="row">
  {{#each projects}}
  <div class="col-md-4 mb-4">
    <div class="card h-100">
      <a href="/project/{{_id}}" target="_blank">
        <img src="{{projectPhoto}}" alt="{{projectName}} Cover Photo" class="card-img-top" style="max-height: 500px; max-width: 500px;">
      </a>
       <div class="card-body">
          <h1 class="card-title">{{projectName}}</h1>
          <form action="/project/{{_id}}" method="GET" class="mt-3">
          <button class="btn btn-info">See Project Details</button>
          </form>
        </div>
      <div class="card-footer">
      </div>
    </div>
  </div>
  {{/each}}
</div>

</section>


